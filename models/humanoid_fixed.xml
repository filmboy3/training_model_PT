<mujoco model="humanoid_squat">
  <compiler angle="radian" coordinate="local" inertiafromgeom="true"/>
  
  <default>
    <joint armature="1" damping="1" limited="true"/>
    <geom conaffinity="0" condim="3" density="5.0" friction="1 0.5 0.5" margin="0.01" rgba="0.8 0.6 0.4 1"/>
    <motor ctrllimited="true" ctrlrange="-1 1"/>
  </default>
  
  <asset>
    <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
    <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
    <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
    <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
    <material name="geom" texture="texgeom" texuniform="true"/>
  </asset>
  
  <worldbody>
    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="20 20 0.125" type="plane"/>
    
    <body name="torso" pos="0 0 1.4">
      <camera name="track" mode="trackcom" pos="0 -3 0.3" xyaxes="1 0 0 0 0 1"/>
      <geom name="torso1" pos="0 0 0.1" size="0.15 0.1" type="capsule"/>
      <geom name="head" pos="0 0 0.19" size="0.09" type="sphere"/>
      <geom name="uwaist" pos="0 0 0.01" size="0.06 0.05" type="capsule"/>
      <joint armature="0" damping="0" limited="false" margin="0.01" name="root" pos="0 0 0" type="free"/>
      
      <!-- Left arm -->
      <body name="larm" pos="0 0.18 0.14">
        <geom name="larm1" pos="0 0 0" size="0.031 0.08" type="capsule"/>
        <joint axis="2 1 1" name="larm" pos="0 0 0" range="0 0 0 0 0 0" type="ball"/>
        <body name="lforearm" pos="0 0.16 0">
          <geom name="lforearm" pos="0 0 0" size="0.027 0.08" type="capsule"/>
          <joint axis="0 0 1" name="lforearm" pos="0 0 0" range="-90 50" type="hinge"/>
        </body>
      </body>
      
      <!-- Right arm -->
      <body name="rarm" pos="0 -0.18 0.14">
        <geom name="rarm1" pos="0 0 0" size="0.031" type="capsule"/>
        <joint axis="2 -1 1" name="rarm" pos="0 0 0" range="-85 60" type="ball"/>
        <body name="rforearm" pos="0 -0.16 0">
          <geom name="rforearm" pos="0 0 0" size="0.027" type="capsule"/>
          <joint axis="0 0 1" name="rforearm" pos="0 0 0" range="-90 50" type="hinge"/>
        </body>
      </body>
      
      <!-- Left leg -->
      <body name="lhip" pos="0 0.1 -0.04">
        <geom name="lhip" pos="0 0 0" size="0.046" type="capsule"/>
        <joint axis="0 0 1" name="lhip_z" pos="0 0 0" range="-45 45" type="hinge"/>
        <joint axis="0 1 0" name="lhip_y" pos="0 0 0" range="-110 20" type="hinge"/>
        <joint axis="1 0 0" name="lhip_x" pos="0 0 0" range="-25 25" type="hinge"/>
        <body name="lthigh" pos="0 0 -0.225">
          <geom name="lthigh1" pos="0 0 0" size="0.046" type="capsule"/>
          <joint axis="0 1 0" name="lknee" pos="0 0 0.02" range="-160 -2" type="hinge"/>
          <body name="lshin" pos="0 0 -0.25">
            <geom name="lshin1" pos="0 0 0" size="0.046" type="capsule"/>
            <joint axis="0 1 0" name="lankle_y" pos="0 0 -0.1" range="-50 50" type="hinge"/>
            <body name="lfoot" pos="0 0 -0.1">
              <geom name="lfoot" pos="0.06 0 0.1" size="0.075 0.05 0.02" type="box"/>
            </body>
          </body>
        </body>
      </body>
      
      <!-- Right leg -->
      <body name="rhip" pos="0 -0.1 -0.04">
        <geom name="rhip" pos="0 0 0" size="0.046" type="capsule"/>
        <joint axis="0 0 1" name="rhip_z" pos="0 0 0" range="-45 45" type="hinge"/>
        <joint axis="0 1 0" name="rhip_y" pos="0 0 0" range="-110 20" type="hinge"/>
        <joint axis="1 0 0" name="rhip_x" pos="0 0 0" range="-25 25" type="hinge"/>
        <body name="rthigh" pos="0 0 -0.225">
          <geom name="rthigh1" pos="0 0 0" size="0.046" type="capsule"/>
          <joint axis="0 1 0" name="rknee" pos="0 0 0.02" range="-160 -2" type="hinge"/>
          <body name="rshin" pos="0 0 -0.25">
            <geom name="rshin1" pos="0 0 0" size="0.046" type="capsule"/>
            <joint axis="0 1 0" name="rankle_y" pos="0 0 -0.1" range="-50 50" type="hinge"/>
            <body name="rfoot" pos="0 0 -0.1">
              <geom name="rfoot" pos="0.06 0 0.1" size="0.075 0.05 0.02" type="box"/>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>
  
  <actuator>
    <motor joint="lhip_z" name="lhip_z"/>
    <motor joint="lhip_y" name="lhip_y"/>
    <motor joint="lhip_x" name="lhip_x"/>
    <motor joint="lknee" name="lknee"/>
    <motor joint="lankle_y" name="lankle_y"/>
    <motor joint="rhip_z" name="rhip_z"/>
    <motor joint="rhip_y" name="rhip_y"/>
    <motor joint="rhip_x" name="rhip_x"/>
    <motor joint="rknee" name="rknee"/>
    <motor joint="rankle_y" name="rankle_y"/>
    <motor joint="larm" name="larm"/>
    <motor joint="lforearm" name="lforearm"/>
    <motor joint="rarm" name="rarm"/>
    <motor joint="rforearm" name="rforearm"/>
  </actuator>
</mujoco>
